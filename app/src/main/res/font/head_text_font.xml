<?xml version="1.0" encoding="utf-8"?>
<font-family
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="UnusedAttribute,ResourceCycle"
    >

    <!--ligt-->
    <font
        android:font="@font/esamanru_otf_light"
        android:fontStyle="normal"
        android:fontWeight="300"
        app:font="@font/esamanru_otf_light"
        app:fontStyle="normal"
        app:fontWeight="300"/>

    <!--medium-->
    <font
        android:font="@font/esamanru_otf_medium"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:font="@font/esamanru_otf_medium"
        app:fontStyle="normal"
        app:fontWeight="500"/>

    <!--bold -->
    <font
        android:font="@font/esamanru_otf_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/esamanru_otf_bold"
        app:fontStyle="normal"
        app:fontWeight="700"/>
</font-family>